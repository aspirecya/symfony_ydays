            color: #b7b5b2;
        }

        ::selection {
            background-color: #385476;
            color: #ffffff;
        }

        ::-moz-selection {
            background-color: #385476;
            color: #ffffff;
        }

        input:-webkit-autofill,
        textarea:-webkit-autofill,
        select:-webkit-autofill {
            background-color: #53553a !important;
            color: #e7e6e5 !important;
        }

        ::-webkit-scrollbar {
            background-color: #1d1e1e;
            color: #c2c1bf;
        }

